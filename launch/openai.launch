<launch>
  <arg name="key" default="$(optenv OPENAI_API_KEY)" />
  <arg name="max_tokens" default="256" />
  <arg name="model" default="gpt-3.5-turbo" />
  <arg name="endpoint" default="" />
  <arg name="azure_api_version" default="2024-07-01-preview" />

  <node pkg="openai_ros" type="openai_node.py" name="openai" output="screen">
    <param name="key" value="$(arg key)" />
    <param name="max_tokens" value="$(arg max_tokens)" />
    <param name="model" value="$(arg model)" />
    <param name="endpoint" value="$(arg endpoint)" />
    <param name="azure_api_version" value="$(arg azure_api_version)" />
  </node>  
</launch>
